{
    "name": "webgraphe/phollow",
    "description": "Live PHP Error Tracking",
    "type": "library",
    "version": "0.2.2",
    "config": {
        "platform": {
            "php": "5.6.1"
        }
    },
    "keywords": [
        "php",
        "script",
        "error",
        "tracking",
        "monitoring"
    ],
    "homepage": "https://github.com/webgraphe/phollow",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Philippe Léveillé",
            "email": "jpleveille@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "email": "jpleveille+phollow@gmail.com",
        "issues": "https://github.com/webgraphe/phollow/issues",
        "docs": "https://webgraphe.github.io/phollow"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "react/http": "^0.8.3",
        "cboden/ratchet": "^0.4.1",
        "nikic/fast-route": "^1.3.0",
        "ext-json": "*",
        "ext-posix": "*"
    },
    "suggest": {
        "ext-pcntl": "To support gracefully shutting down the server using process control signals"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    },
    "bin": [
        "phollow"
    ],
    "autoload": {
        "psr-4": {
            "Webgraphe\\Phollow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webgraphe\\Phollow\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit --colors"
    }
}
